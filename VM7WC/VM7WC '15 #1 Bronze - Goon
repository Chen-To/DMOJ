import java.util.*;
public class Main {
  public static void main(String[] args) {
    Scanner in = new Scanner (System.in);
    String phoneNumber = in.nextLine();
    int num1, num2, num3;
    num1 = Integer.parseInt(phoneNumber.substring(0,1)) + Integer.parseInt(phoneNumber.substring(1,2)) + Integer.parseInt(phoneNumber.substring(2,3));
    num2 = Integer.parseInt(phoneNumber.substring(4,5)) + Integer.parseInt(phoneNumber.substring(5,6)) + Integer.parseInt(phoneNumber.substring(6,7));
    num3 = Integer.parseInt(phoneNumber.substring(8,9)) + Integer.parseInt(phoneNumber.substring(9,10)) + Integer.parseInt(phoneNumber.substring(10,11)) + Integer.parseInt(phoneNumber.substring(11,12));
    if (num1 == num2 && num2 == num3)
      System.out.println("Goony!");
    else 
      System.out.println("Pick up the phone!");
    
    }
  }
